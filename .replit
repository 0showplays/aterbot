hidden = [".config"]
run = "npm run start"


[nix]
	channel = "stable-22_11"


[gitHubImport]
	requiredFiles = ["replit.nix", ".config"]


[packager]
	language = "nodejs"

	[packager.features]
		packageSearch = true
		guessImports = true
		enabledForHosting = false


[unitTest]
	language = "nodejs"


[languages]
	[languages.javascript]
		pattern = "**/{*.js,*.jsx,*.ts,*.tsx}"

	[languages.javascript.languageServer]
		start = "typescript-language-server --stdio"


[debugger]
	support = false